		<in_img file_name ="stereo_lena2.bmp"/>
		<in_img file_name ="stereo_lena.bmp"/>
		<in_img file_name ="simple.bmp"/>
    </in_imgs>
    <out_imgs>
        <out_img file_name="processed_img.bmp"/>
    </out_imgs>
</imgdsp>







