<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE imgdsp>
<imgdsp>
    <programs>
        <program name="SIFT Detect">
            <params>
            </params>
        </program>
        <program name="SIFT Detect and match">
            <params>
              <param name="Threshold" min="0" current="1" max="1000"/>
